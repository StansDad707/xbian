	if [ ! -d "/usr/local/lib/xbmc/addons/library.xbmc.gui" ]; then
		mkdir -p "/usr/local/lib/xbmc/addons/library.xbmc.gui";
	fi

	if [ ! -d "/usr/local/lib/xbmc/addons/library.xbmc.pvr" ]; then
		mkdir -p "/usr/local/lib/xbmc/addons/library.xbmc.pvr";
	fi

	if [ ! -d "/usr/local/lib/xbmc/addons/library.xbmc.addon" ]; then
		mkdir -p "/usr/local/lib/xbmc/addons/library.xbmc.addon";
	fi

	if [ ! -d "/usr/local/lib/xbmc/addons/visualization.glspectrum" ]; then
		mkdir -p "/usr/local/lib/xbmc/addons/visualization.glspectrum";
	fi

	if [ ! -d "/usr/local/lib/xbmc/addons/visualization.waveform" ]; then
		mkdir -p "/usr/local/lib/xbmc/addons/visualization.waveform";
	fi
	
fi